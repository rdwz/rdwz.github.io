{
  "name": "rdwz.github.io",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "Marc RDWZ",
    "url": "https://github.com/rdwz"
  },
  "homepage": "https://yurdwz.github.io",
  "keywords": [
    "astro"
  ],
  "engines": {
    "node": ">=18.16",
    "pnpm": ">=8.6"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "check": "astro check && npm run lint",
    "lint:check": "prettier --check \"src/**/*.astro\"",
    "lint:fix": "prettier --cache --write \"src/**/*.astro\"",
    "prepare": "husky install",
    "preview": "pnpm install && astro preview && vercel"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "pnpm dlx prettier --cache --write \"src/**/*.astro\""
    ]
  },
  "dependencies": {
		"astro": "^3.0.10"
	},
  "devDependencies": {
		"@astrojs/check": "^0.2.0",
		"astro-compress": "^2.0.15",
    "astro-compressor": "^0.4.0",
    "astro-emoji": "^1.2.0",
    "astro-icon": "^0.8.1",
    "astro-robots-txt": "^0.5.0",
    "astro-sitemap": "^0.6.0",
    "autoprefixer": "^10.4.15",
    "husky": "^8.0.3",
    "libravatar": "^3.0.0",
    "lint-staged": "^14.0.1",
    "open-props": "^1.5.15",
    "postcss": "^8.4.29",
    "postcss-jit-props": "^1.0.13",
    "postcss-preset-env": "^9.1.3",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
		"typescript": "^5.2.2"
  }
}
