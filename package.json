{
  "name": "rdwz.github.io",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "Marc RDWZ",
    "url": "https://github.com/rdwz"
  },
  "homepage": "https://marc.is-a-fullstack.dev",
  "keywords": [
    "astro"
  ],
  "engines": {
    "node": ">=18.16.1"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "check": "astro check && nr lint",
    "lint": "prettier --check \"src/**/*.astro\"",
    "format": "prettier --cache --write \"src/**/*.astro\"",
    "postinstall": "cd node_modules/sharp && ni",
    "prepare": "husky install",
    "preview": "ni && astro preview && vercel"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "pnpm dlx prettier --cache --write \"src/**/*.astro\""
    ]
  },
  "dependencies": {
		"astro": "^3.6.4",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
		"@antfu/ni": "^0.21.12",
		"@astrojs/check": "^0.3.1",
    "@astrojs/partytown": "^2.0.2",
    "astro-beam": "^1.1.1",
    "astro-compress": "^2.2.3",
    "astro-compressor": "^0.4.1",
    "astro-emoji": "^1.2.0",
    "astro-icon": "^0.8.1",
    "astro-robots-txt": "^1.0.0",
    "astro-sitemap": "^1.0.0",
    "autoprefixer": "^10.4.16",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "more-modern-css-reset": "^1.0.0",
    "open-props": "^1.6.13",
    "postcss-jit-props": "^1.0.14",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.13.0"
  }
}
